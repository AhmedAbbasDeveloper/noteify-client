{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "jsx-a11y/no-autofocus": "off",
    "linebreak-style": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-no-constructed-context-values": "off",
    "react/prop-types": "off",
    "no-underscore-dangle": "off"
  }
}
