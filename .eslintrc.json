{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "react/jsx-filename-extension": "off",
    "react/jsx-no-constructed-context-values": "off",
    "react/prop-types": "off"
  }
}
